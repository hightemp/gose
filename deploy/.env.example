# Docker Compose defaults (used by deploy/docker-compose.yml)
POSTGRES_USER=search
POSTGRES_PASSWORD=search
POSTGRES_DB=search

# Host ports mapping (can be changed to avoid conflicts)
PG_PORT=5433
CRAWLER_PORT=18082
SEARCH_UI_PORT=18080

# Connection string for services that read DB DSN from env (inside Compose network host is "postgres")
PG_DSN=postgres://search:search@postgres:5432/search?sslmode=disable

# Manager UI host port mapping
MANAGER_UI_PORT=18083